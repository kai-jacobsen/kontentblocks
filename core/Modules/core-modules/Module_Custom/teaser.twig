<div class="featurette--wrapper grunge">
    <div class="row">
        {% for item in items %}
            <div class="small-4 large-4 columns featurette featurette__center">
                <img class="circle" src="{{ item.circle }}" >
                {{ item.content.el('div').html() | raw }}
            </div>
        {% endfor %}
    </div>
</div>
